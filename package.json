  },
  "homepage": "https://github.com/fg123/replication#readme",
  "dependencies": {
    "express": "^4.17.1",
    "path-browserify": "^1.0.1",
    "webpack-merge": "^5.7.2",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
